				  <td>
					  <button type="button" title="Guardar" id="gl_<%= ID %>" onclick="guardarEditado(this.id)">o</button>
					  <button type="button" title="Borrar" id="bl_<%= ID %>"onclick="borrar(this.id)">X</button>
				  </td>
				</tr>
			 <% } %>
		  </table>
