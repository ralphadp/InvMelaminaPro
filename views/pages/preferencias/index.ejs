	  <div id="fondo" class="container_table">
		<span class="form-label"><h5>Fondo   <button type="button" onclick="add_row()">+</button></h5></span>
		<div class="table-editable">
		  <table class="table" id="fondo_data_table">
			<tr>
			  <th> Color </th>  
			  <th> Provedor </th>  
